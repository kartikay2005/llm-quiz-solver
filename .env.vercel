# Vercel-specific .env template
# Copy this to .env and fill in your values

# Required
QUIZ_SECRET=your_jwt_token_here
STUDENT_EMAIL=your_email@example.com

# Optional (for OpenAI fallback)
OPENAI_API_KEY=sk-your_openai_key_here
AIPIPE_API_KEY=your_aipipe_key_here

# Vercel automatically sets this
# VERCEL=1

# Playwright settings (ignored in Vercel)
PLAYWRIGHT_HEADLESS=True
DOWNLOAD_DIR=.downloads
LOG_LEVEL=INFO
